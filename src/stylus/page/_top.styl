.top__main
  overflow-x hidden
  position relative
  text-align center
  z-index 0

.mask.active
  width 100%
  height 100%
  position fixed
  z-index 8
  background rgba(0,0,0,0.55)
  transition ease .4s

@keyframes upDown {
  0% {
    transform: translateY(-8%)
  }
  15% {
    transform: translateY(8%)
  }
  30% {
    transform: translateY(-5%)
  }
  45% {
    transform: translateY(0%)
  }
}

